{
  "name": "youtube-analysis-backend",
  "version": "0.1.0",
  "description": "Node.js REST API backend for the YouTube analysis frontend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "echo \"No tests configured\"",
    "sync:spotlight": "ts-node-dev --transpile-only src/workers/syncSpotlightChannels.ts",
    "sync:spotlight:prod": "node dist/workers/syncSpotlightChannels.js",
    "sync:subscriptions": "ts-node-dev --transpile-only src/workers/syncSubscribedChannels.ts",
    "sync:subscriptions:prod": "node dist/workers/syncSubscribedChannels.js"
  },
  "keywords": [
    "node",
    "express",
    "rest",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "type": "commonjs",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "minio": "^8.0.6",
    "pg": "^8.16.3",
    "undici": "^5.29.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "@types/pg": "^8.15.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
